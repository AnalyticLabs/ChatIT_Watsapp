{
  "expo": {
    "name": "Chatit",
    "slug": "Chatit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Chatit.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Chatit.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Chatit to access your camera.",
        "NSPhotoLibraryUsageDescription": "Allow Chatit to access your photo library."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Chatit.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Chatit",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Chatit.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
