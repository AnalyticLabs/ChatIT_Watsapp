{
  "expo": {
    "name": "Chatit",
    "slug": "chatit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Chatit.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/Chatit.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Chatit to access your camera.",
        "NSPhotoLibraryUsageDescription": "Allow Chatit to access your photo library.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.anonymous.Chatit"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Chatit.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Chatit",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Chatit.png"
    },
    "plugins": ["expo-router", "expo-image-picker"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2f230033-30ea-473b-a7c8-352e18f838cb"
      },
      "backendUrl": "https://chatit-backend-zhlk.onrender.com/api"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2f230033-30ea-473b-a7c8-352e18f838cb"
    },
    "owner": "chatit"
  }
}
